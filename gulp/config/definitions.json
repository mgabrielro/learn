{
  "compileLESS": {
    "group_key": "LESS",
    "styles": {
      "src": [
        "./public/assets/desktop/less/main.less"
      ],
      "lib": [
        "./public/assets/desktop/less/**/*.less"
      ],
      "target": {
        "path": "./public/assets/desktop/dist/css",
        "name": "app.css"
      }
    }
  },
  "compileJS": {
    "group_key": "JS",
    "libsJS": {
      "src": [
        "./bower_components/jquery/dist/jquery.min.js",
        "./bower_components/jquery.popup.overlay/jquery.popupoverlay.js",
        "./bower_components/spin.js/spin.min.js",
        "./bower_components/svg4everybody/dist/svg4everybody.min.js",
        "./public/assets/desktop/js/constants/constants.js",
        "./public/assets/desktop/js/libs/*.js"
      ],
      "target": {
        "path": "./public/assets/desktop/dist/js",
        "name": "libs.js"
      }
    },
    "appJS": {
      "src": [
        "./public/assets/desktop/js/domElements/*.js",
        "./public/assets/desktop/js/formElements/*.js",
        "!./public/assets/desktop/js/efeedback.js",
        "!./public/assets/desktop/js/favorite.js",
        "!./public/assets/desktop/js/functions.js",
        "./public/assets/desktop/js/svg-icons.js",
        "./public/assets/desktop/js/tooltip.js",
        "./public/assets/desktop/js/utils.js"
      ],
      "target": {
        "path": "./public/assets/desktop/dist/js",
        "name": "app.js"
      }
    }
  }
}